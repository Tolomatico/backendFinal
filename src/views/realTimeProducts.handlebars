<h1>{{title}}</h1>

<div id="productsRealTime" class="container"></div>

<form ></form>

    <legend class="legend">Formulario</legend>

    <div class="form">

        <input class="form-input" type="text" id="title" placeholder="Titulo">
        <input class="form-input"type="text" id="description" placeholder="Descripción">
        <input class="form-input"type="number" id="price" placeholder="Precio">
        <input class="form-input"type="text" id="img" value="Sin Imagen">
        <input class="form-input"type="file" id="thumbnail" value="Sin Imagen">
        <input class="form-input"type="text" id="code" placeholder="Código">
        <input class="form-input" type="number" id="stock" placeholder="Stock">
        <input class="form-input" type="text" id="category" placeholder="Categoria">
        <input class="hidden" type="email" id="owner" value="{{user.email}}">
        <select class="form-input" id="status">
            <option value="true">Activo</option>
            <option value="false">Inactivo</option>
        </select>
        <button class="form-input border-2 rounded-xl border-blue-700 bg-blue-700 text-white p-1 m-1" type="button" id="btnSend">Enviar</button>
    </div>
    
</form>






<script src="https://cdn.socket.io/4.7.3/socket.io.min.js"
    integrity="sha384-+miq58Ltmjm+SSP5g8Ok6dpMO/6R4n8xe/x1Yj0Bdb9kiEz25LTnWv/GZQTRQamg"
    crossorigin="anonymous"></script>
<script src="js/index.js"></script>